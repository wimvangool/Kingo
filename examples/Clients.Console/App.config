<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <system.serviceModel>
    <client>
      <endpoint name="ChallengeService_NetTcp" address="net.tcp://localhost:1002/Challenges" binding="netTcpBinding" contract="Kingo.Samples.Chess.Challenges.IChallengeService" />

      <endpoint name="GameService_NetTcp" address="net.tcp://localhost:1002/Games" binding="netTcpBinding" contract="Kingo.Samples.Chess.Games.IGameService" />

      <endpoint name="PlayerService_NetTcp" address="net.tcp://localhost:1002/Players" binding="netTcpBinding" contract="Kingo.Samples.Chess.Players.IPlayerService" />
    </client>
    <behaviors>
      <endpointBehaviors>
        <behavior>
          <SessionBehavior />
        </behavior>        
      </endpointBehaviors>
    </behaviors>
    <extensions>
      <behaviorExtensions>
        <add name="SessionBehavior" type="Kingo.Samples.Chess.SessionBehavior+ExtensionElement, Kingo.Samples.Chess.Api" />
      </behaviorExtensions>
    </extensions>
  </system.serviceModel>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Kingo" publicKeyToken="cbd9f157c5e464e2" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.1.0.0" newVersion="3.1.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
